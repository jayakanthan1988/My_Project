COLUMN TABLE "STUDENTMASTER" (
"STUDENTID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 100 INCREMENT BY 1 MAXVALUE 300) NOT NULL COMMENT 'Student ID',
"STUDENTNAME" NVARCHAR(25) COMMENT 'Student Name',
"DEPARTMENT" NVARCHAR(25) COMMENT 'Department',
"YEAR" NVARCHAR(10) COMMENT 'Year',
"JOINDATE" DATE CS_DAYDATE COMMENT 'Joined Date',
PRIMARY KEY ("STUDENTID") )
COMMENT 'Student Master Table' 